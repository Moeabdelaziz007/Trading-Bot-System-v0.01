# TRADING SYSTEM 0.1 - Environment Variables
# Copy this to .env and fill in your keys

# Demo Mode (set to false for live trading)
DEMO_MODE=true

# Alpaca API (Paper Trading)
# Get keys from: https://app.alpaca.markets/paper/dashboard
ALPACA_KEY=your_alpaca_key_here
ALPACA_SECRET=your_alpaca_secret_here
ALPACA_ENDPOINT=https://paper-api.alpaca.markets

# Binance API (for real crypto trading)
BINANCE_KEY=
BINANCE_SECRET=

# OpenRouter API (Multi-Model AI Gateway)
# Get keys from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_key_here

# AlphaAPI Gateway (Signal Distribution)
ALPHA_GATEWAY_URL=https://api.alphaaxiom.com/api/v1/signals/push
ALPHA_INTERNAL_TOKEN=your_internal_token_here
